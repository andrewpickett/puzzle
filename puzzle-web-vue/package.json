{
  "name": "puzzle-web-vue",
  "description": "Simple puzzle application",
  "version": "1.0.0",
  "author": "Andrew J. Pickett",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start:dev": "cross-env NODE_ENV=development webpack-dev-server --hot --watch-poll --open --config webpack.dev.js",
    "start:prod": "cross-env NODE_ENV=production webpack-dev-server --hot --watch-poll --open --config webpack.prod.js",
    "build:dev": "cross-env NODE_ENV=development webpack --progress --hide-modules --watch --config webpack.dev.js",
    "build:prod": "cross-env NODE_ENV=production webpack --progress --hide-modules --config webpack.prod.js"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "bootstrap": "^3.3.7",
    "jquery": "^3.2.1",
    "moment": "^2.19.3",
    "showdown": "^1.8.3",
    "vue": "^2.4.4",
    "vue-router": "^3.0.1",
    "vue2-filters": "^0.2.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "copy-webpack-plugin": "^4.2.3",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "file-loader": "^1.1.4",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "vue-loader": "^13.0.5",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1",
    "webpack-merge": "^4.1.1"
  }
}
